import java.lang.*;
import java.util.*;

class Solution {
    public String solution(String s) {
        StringBuilder sb = new StringBuilder();
        String[] tempStringArray = s.split(" ");
        ArrayList<Long> tempArrayList = new ArrayList<Long>();
        
        // 정수의 길이가 정해져 있지 않으므로, Long으로 변환하여 ArrayList에 add한다.
        // String으로 정렬을 했을 경우 음수 정렬이 정상적으로 되지 않아 정수형 타입으로 변환하였다.
        for(int i = 0; i < tempStringArray.length; i++) {
            tempArrayList.add(Long.parseLong(tempStringArray[i]));
        }
        
        Collections.sort(tempArrayList);
        
        sb.append(tempArrayList.get(0) + " ");
        sb.append(tempArrayList.get(tempArrayList.size() - 1));
        
        return sb.toString();
    }
}
