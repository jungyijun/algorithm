class Solution {
    public int solution(int n) {
        int answer = 0;
        
        // 피보나치 수의 처음을 초기화 및 선언한다.
        // a는 n-2 의 값을 의미하는 변수이며,
        // b는 n-1 의 값을 의미하는 변수이다.
        int a = 1;
        int b = 1;
        
        for(int i = 2; i < n; i++) {
            answer = (a + b) % 1234567;
            a = b;
            b = answer;
        }
        
        return answer;
    }
}
