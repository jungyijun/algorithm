import java.util.*;

class Solution {
    public int solution(int[] nums) {
        int answer = 0;
        Set<Integer> treeSet = new TreeSet<Integer>();
        
        for(int i = 0; i < nums.length; i++) {
            treeSet.add(nums[i]);
        }
        
        if((nums.length / 2) < treeSet.size()) {
            answer = nums.length / 2;
        } else {
            answer = treeSet.size();
        }
        
        return answer;
    }
}
